#include <elements/local/RfFrontEnd.hh>
#include <elements/local/RFFrontEndWifi.cc>
class RfFrontEndFactory{ public:

	RfFrontEndFactory(){
	}
	~RfFrontEndFactory(){
	}
	
	RfFrontEnd* getRfFrontEnd (String type){
	RfFrontEnd* rf;
		if (type == "wifi")
			rf = new RFFrontEndWifi();
		return rf;
	}
};
